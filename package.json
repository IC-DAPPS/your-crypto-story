{
  "name": "sveltekit-motoko-starter",
  "version": "0.0.1",
  "private": true,
  "description": "Internet Computer SvelteKit starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister",
    "SvelteKit"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test --project=chromium",
    "test:card": "playwright test card.spec.ts --headed --project=chromium",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.45.3",
    "@rollup/plugin-inject": "^5.0.5",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "8.56.0",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.19",
    "buffer": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "flowbite": "^2.4.1",
    "flowbite-svelte": "^0.46.1",
    "flowbite-svelte-icons": "^1.6.1",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-motoko": "^0.8.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.4.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-tailwind-purgecss": "^0.3.3"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^1.0.0",
    "@dfinity/auth-client": "^1.0.0",
    "@dfinity/candid": "^1.4.0",
    "@dfinity/ledger-icrc": "^2.4.0",
    "@dfinity/principal": "^1.4.0",
    "@dfinity/utils": "^2.4.0"
  }
}
